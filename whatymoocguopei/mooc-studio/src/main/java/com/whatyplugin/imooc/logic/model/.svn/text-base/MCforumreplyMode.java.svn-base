package com.whatyplugin.imooc.logic.model;

public class MCforumreplyMode {

	private String id;
	private String parentId;
	private String detail;
	private String replyNickname;
	private String nickName;
	private String replyUserId;
	private String mainId;
	private String userId;
	private String picFileUrl;
	private String postTime;
	private String temp_draft;
	private boolean isOnlyReply;

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getParentId() {
		return parentId;
	}

	public void setParentId(String parentId) {
		this.parentId = parentId;
	}

	public String getDetail() {
		return detail;
	}

	public void setDetail(String detail) {
		this.detail = detail;
	}

	public String getReplyNickname() {
		return replyNickname;
	}

	public void setReplyNickname(String replyNickname) {
		this.replyNickname = replyNickname;
	}

	public String getNickName() {
		return nickName;
	}

	public void setNickName(String nickName) {
		this.nickName = nickName;
	}

	public String getReplyUserId() {
		return replyUserId;
	}

	public void setReplyUserId(String replyUserId) {
		this.replyUserId = replyUserId;
	}

	public String getMainId() {
		return mainId;
	}

	public void setMainId(String mainId) {
		this.mainId = mainId;
	}

	public String getUserId() {
		return userId;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	public String getPicFileUrl() {
		return picFileUrl;
	}

	public void setPicFileUrl(String picFileUrl) {
		this.picFileUrl = picFileUrl;
	}

	public String getPostTime() {
		return postTime;
	}

	public void setPostTime(String postTime) {
		this.postTime = postTime;
	}

	public boolean isOnlyReply() {
		return isOnlyReply;
	}

	public void setOnlyReply(boolean isOnlyReply) {
		this.isOnlyReply = isOnlyReply;
	}

	public String getTemp_draft() {
		return temp_draft;
	}

	public void setTemp_draft(String temp_draft) {
		this.temp_draft = temp_draft;
	}

}
