<?xml version="1.0" encoding="UTF-8"?>
<selector android:constantSize="true"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="#f1f1f1" android:endColor="#f1f1f1" android:angle="270.0"/>
        </shape>
    </item>
    
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="#f9f9f9" android:endColor="#f9f9f9" android:angle="270.0"/>
        </shape>
    </item>
</selector>