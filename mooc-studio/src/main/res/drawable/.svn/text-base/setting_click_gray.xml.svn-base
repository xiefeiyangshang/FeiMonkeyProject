<?xml version="1.0" encoding="UTF-8"?>
<selector android:constantSize="true"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="#cfcfcf" android:endColor="#cfcfcf" android:angle="270.0"/>
        </shape>
    </item>
    
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="#f5f5f5" android:endColor="#f5f5f5" android:angle="270.0"/>
        </shape>
    </item>
</selector>