package com.whatyplugin.base.weblog;

public class BeanScreen {
	public int width;
	public int height;
	public int colorDepth;
	public BeanScreen setWidth(int width) {
		this.width = width;
		return this;
	}
	public BeanScreen setHeight(int height) {
		this.height = height;
		return this;
	}
	public BeanScreen setColorDepth(int colorDepth) {
		this.colorDepth = colorDepth;
		return this;
	}
	
	
}
